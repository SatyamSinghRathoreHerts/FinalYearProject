       "\u001b[0m        \u001b[0moutputs\u001b[0m\u001b[1;33m=\u001b[0m\u001b[1;33m[\u001b[0m\u001b[1;33m\n",
       "\u001b[0m            \u001b[1;34m'pose_landmarks'\u001b[0m\u001b[1;33m,\u001b[0m \u001b[1;34m'pose_world_landmarks'\u001b[0m\u001b[1;33m,\u001b[0m \u001b[1;34m'left_hand_landmarks'\u001b[0m\u001b[1;33m,\u001b[0m\u001b[1;33m\n",
